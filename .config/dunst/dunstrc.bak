[global]
font = Iosevka 11
markup = yes
format = "%s\n%b"
sort = yes
indicate_hidden = yes
alignment = center
word_wrap = yes
stack_duplicates = yes
hide_duplicates_count = no
geometry = "300x50-15+49"
shrink = yes
transparency = 20
follow = none
show_indicators = no
separator_height = 0
padding = 15
horizontal_padding = 15
separator_color = frame
startup_notification = false
dmenu = /usr/bin/dmenu -p dunst:
browser = /usr/bin/qutebrowser
frame_width = 2
frame_color = "#8EC07C"

[shortcuts]
close = ctrl+space
close_all = ctrl+shift+space
context = ctrl+shift+f
history = ctrl+shift+d

[urgency_low]

frame_color = "#e5e5e5"
foreground = "#ffffff"
background = "#000000"
timeout = 4

[urgency_normal]
frame_color = "#339562"
foreground = "#ffffff"
background = "#000000"
timeout = 6

[urgency_critical]
frame_color = "cc0403"
foreground = "#ffffff"
background = "#000000"
timeout = 20
